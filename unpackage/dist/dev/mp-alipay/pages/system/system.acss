
.tab-box {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	height: 100rpx;
	width: 100%;
	background: #ffffff;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.tab-container {
	width: 500rpx;
	height: 100%;
	height: 74rpx;
	-webkit-align-self: center;
	        align-self: center;
	border-radius: 10rpx;
	border: solid #000000 1rpx;
}

