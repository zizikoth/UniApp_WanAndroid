
.banner-container {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	height: 400rpx;
	width: 100%;
}
.banner-box {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	height: 400rpx;
}
.banner-image {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: calc(100% - 20rpx);
	margin: 20rpx;
	height: 360rpx;
	border-radius: 20rpx;
}
 .uni-swiper-dots {
	bottom: 40rpx !important;
}

