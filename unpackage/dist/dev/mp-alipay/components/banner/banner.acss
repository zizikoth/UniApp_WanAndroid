
.banner-container {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	height: 360rpx;
	width: 100%;
}
.banner-box {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	height: 360rpx;
}
.banner-image {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: calc(100% - 30rpx);
	margin-right: 15rpx;
	margin-left: 15rpx;
	height: 360rpx;
	border-radius: 20rpx;
}

