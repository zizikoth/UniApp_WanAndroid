<block a:if="{{data!=undefined&&data!=null&&data.length!=0}}"><view class="banner-container"><swiper class="banner-box" circular="{{true}}" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{4000}}" previous-margin="30rpx" next-margin="30rpx" indicator-color="#46485F" indicator-active-color="#F58F98" duration="{{1000}}" current="{{position}}" data-event-opts="{{[['change',[['change',['$event']]]]]}}" onChange="__e"><swiper-item a:for="{{data}}" a:for-item="item" a:for-index="index" a:key="index"><image class="banner-image" src="{{item.image}}" mode="aspectFill" data-event-opts="{{[['tap',[['itemClick',['$0'],[[['data','',index]]]]]]]}}" onTap="__e"></image></swiper-item></swiper></view></block>