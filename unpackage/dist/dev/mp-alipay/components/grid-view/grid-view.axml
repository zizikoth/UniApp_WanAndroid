<block a:if="{{data!=undefined&&data!=null&&data.length>0}}"><view class="grid-view-container"><view data-event-opts="{{[['tap',[['itemClick',['$0'],[[['data','',index]]]]]]]}}" class="grid-view-item" onTap="__e" a:for="{{data}}" a:for-item="item" a:for-index="index" a:key="index"><image class="grid-view-item-icon" src="{{item.image}}"></image><text class="grid-view-item-title">{{item.title}}</text></view></view></block>