
.tip-container {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	width: 500rpx;
	border-radius: 20rpx;
	background: #ffffff;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-align-self: center;
	        align-self: center;
}
.tip-title {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	text-align: center;
	font-size: 30rpx;
	font-weight: bold;
	margin-top: 30rpx;
	margin-left: 30rpx;
	margin-right: 30rpx;
	color: #333333;
}
.tip-content {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	text-align: center;
	font-size: 28rpx;
	margin: 30rpx;
	font-weight: bold;
	color: #666666;
}
.tip-line-h {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	height: 2rpx;
	background: #F5F5F5;
}
.tip-tap-container {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	width: 100%;
	height: 80rpx;
}
.tip-line-v {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 2rpx;
	height: 100%;
	background: #F5F5F5;
}
.tip-tap-text {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	font-size: 28rpx;
	color: #666666;
	text-align: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}


