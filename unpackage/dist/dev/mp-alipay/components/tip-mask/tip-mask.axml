<view class="tip-container"><view class="tip-title">{{title}}</view><view class="tip-content">{{content}}</view><view class="tip-line-h"></view><view class="tip-tap-container"><text data-event-opts="{{[['tap',[['onNegative',['$event']]]]]}}" class="tip-tap-text" onTap="__e">{{negative}}</text><view class="tip-line-v"></view><text data-event-opts="{{[['tap',[['onPositive',['$event']]]]]}}" class="tip-tap-text" onTap="__e">{{positive}}</text></view></view>