<view><view class="rank-container" a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="userId"><view class="rank-rank-box"><block a:if="{{item.rank==1}}"><image class="rank-rank-image" src="/static/ic_first.png" mode="aspectFit"></image></block><block a:if="{{item.rank==2}}"><image class="rank-rank-image" src="/static/ic_second.png" mode="aspectFit"></image></block><block a:if="{{item.rank==3}}"><image class="rank-rank-image" src="/static/ic_third.png" mode="aspectFit"></image></block><block a:if="{{item.rank>3}}"><text class="rank-text">{{item.rank}}</text></block></view><text class="rank-name">{{item.username}}</text><text class="rank-text">{{item.coinCount}}</text></view></view>